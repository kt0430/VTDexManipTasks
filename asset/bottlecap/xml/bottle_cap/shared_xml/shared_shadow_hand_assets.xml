  <contact>
    <exclude body1="rh_wrist" body2="rh_forearm"></exclude>
    <exclude body1="rh_thproximal" body2="rh_thmiddle"></exclude>
  </contact>

  <tendon>
    <fixed name="rh_FFJ0">
      <joint joint="rh_FFJ2" coef="1"></joint>
      <joint joint="rh_FFJ1" coef="1"></joint>
    </fixed>
    <fixed name="rh_MFJ0">
      <joint joint="rh_MFJ2" coef="1"></joint>
      <joint joint="rh_MFJ1" coef="1"></joint>
    </fixed>
    <fixed name="rh_RFJ0">
      <joint joint="rh_RFJ2" coef="1"></joint>
      <joint joint="rh_RFJ1" coef="1"></joint>
    </fixed>
    <fixed name="rh_LFJ0">
      <joint joint="rh_LFJ2" coef="1"></joint>
      <joint joint="rh_LFJ1" coef="1"></joint>
    </fixed>
  </tendon>

  <actuator>
    <position name="rh_A_WRJ2" joint="rh_WRJ2" class="wrist_y"></position>
    <position name="rh_A_WRJ1" joint="rh_WRJ1" class="wrist_x"></position>
    <position name="rh_A_THJ5" joint="rh_THJ5" class="thbase"></position>
    <position name="rh_A_THJ4" joint="rh_THJ4" class="thproximal"></position>
    <position name="rh_A_THJ3" joint="rh_THJ3" class="thhub"></position>
    <position name="rh_A_THJ2" joint="rh_THJ2" class="thmiddle"></position>
    <position name="rh_A_THJ1" joint="rh_THJ1" class="thdistal"></position>
    <position name="rh_A_FFJ4" joint="rh_FFJ4" class="knuckle"></position>
    <position name="rh_A_FFJ3" joint="rh_FFJ3" class="proximal"></position>
    <position name="rh_A_FFJ0" tendon="rh_FFJ0" class="middle_distal"></position>
    <position name="rh_A_MFJ4" joint="rh_MFJ4" class="knuckle"></position>
    <position name="rh_A_MFJ3" joint="rh_MFJ3" class="proximal"></position>
    <position name="rh_A_MFJ0" tendon="rh_MFJ0" class="middle_distal"></position>
    <position name="rh_A_RFJ4" joint="rh_RFJ4" class="knuckle"></position>
    <position name="rh_A_RFJ3" joint="rh_RFJ3" class="proximal"></position>
    <position name="rh_A_RFJ0" tendon="rh_RFJ0" class="middle_distal"></position>
    <position name="rh_A_LFJ5" joint="rh_LFJ5" class="metacarpal"></position>
    <position name="rh_A_LFJ4" joint="rh_LFJ4" class="knuckle"></position>
    <position name="rh_A_LFJ3" joint="rh_LFJ3" class="proximal"></position>
    <position name="rh_A_LFJ0" tendon="rh_LFJ0" class="middle_distal"></position>
  </actuator>
  <sensor>

    <touch name="f_lfdistal" site="fs_lfdistal" noise="0.001" cutoff="10"></touch>
    <touch name="f_rfdistal" site="fs_rfdistal" noise="0.001" cutoff="10"></touch>
    <touch name="f_mfdistal" site="fs_mfdistal" noise="0.001" cutoff="10"></touch>
    <touch name="f_ffdistal" site="fs_ffdistal" noise="0.001" cutoff="10"></touch>
    <touch name="f_thdistal" site="fs_thdistal" noise="0.001" cutoff="10"></touch>

    <touch name="f_lfmiddle" site="fs_lfmiddle" noise="0.001" cutoff="10"></touch>
    <touch name="f_rfmiddle" site="fs_rfmiddle" noise="0.001" cutoff="10"></touch>
    <touch name="f_mfmiddle" site="fs_mfmiddle" noise="0.001" cutoff="10"></touch>
    <touch name="f_ffmiddle" site="fs_ffmiddle" noise="0.001" cutoff="10"></touch>
    <touch name="f_thmiddle" site="fs_thmiddle" noise="0.001" cutoff="10"></touch>

    <touch name="f_lfproximal" site="fs_lfproximal" noise="0.001" cutoff="10"></touch>
    <touch name="f_rfproximal" site="fs_rfproximal" noise="0.001" cutoff="10"></touch>
    <touch name="f_mfproximal" site="fs_mfproximal" noise="0.001" cutoff="10"></touch>
    <touch name="f_ffproximal" site="fs_ffproximal" noise="0.001" cutoff="10"></touch>
    <touch name="f_thproximal" site="fs_thproximal" noise="0.001" cutoff="10"></touch>

    <touch name="f_lfknuckle" site="fs_lfknuckle" noise="0.001" cutoff="10"></touch>
    <touch name="f_rfknuckle" site="fs_rfknuckle" noise="0.001" cutoff="10"></touch>
    <touch name="f_mfknuckle" site="fs_mfknuckle" noise="0.001" cutoff="10"></touch>
    <touch name="f_ffknuckle" site="fs_ffknuckle" noise="0.001" cutoff="10"></touch>
    <touch name="f_palm" site="fs_palm" noise="0.001" cutoff="10"></touch>

  </sensor>
</mujoco>
