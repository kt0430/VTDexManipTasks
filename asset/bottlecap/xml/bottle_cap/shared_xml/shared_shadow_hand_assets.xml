<mujoco model="right_shadow_hand_assets">
  <size njmax="500" nconmax="100" nuser_jnt="1" nuser_site="1" nuser_tendon="1" nuser_sensor="1" nuser_actuator="16" nstack="600000"></size>

    <visual>
        <map fogstart="3" fogend="5" force="0.1"></map>
        <quality shadowsize="4096"></quality>
    </visual>

  <default>
    <default class="right_hand">
      <mesh scale="0.001 0.001 0.001"></mesh>
      <joint axis="1 0 0" damping="0.05" armature="0.0002" frictionloss="0.01"></joint>
      <position forcerange="-1 1"></position>

      <default class="wrist">
        <joint damping="0.5"></joint>
        <default class="wrist_y">
          <joint axis="0 1 0" range="-0.523599 0.174533"></joint>
          <position kp="10" ctrlrange="-0.523599 0.174533" forcerange="-10 10"></position>
        </default>
        <default class="wrist_x">
          <joint range="-0.698132 0.488692"></joint>
          <position kp="8" ctrlrange="-0.698132 0.488692" forcerange="-5 5"></position>
        </default>
      </default>

      <default class="thumb">
        <default class="thbase">
          <joint axis="0 0 -1" range="-1.0472 1.0472"></joint>
          <position kp="0.4" ctrlrange="-1.0472 1.0472" forcerange="-3 3"></position>
        </default>
        <default class="thproximal">
          <joint range="0 1.22173"></joint>
          <position ctrlrange="0 1.22173" forcerange="-2 2"></position>
        </default>
        <default class="thhub">
          <joint range="-0.20944 0.20944"></joint>
          <position kp="0.5" ctrlrange="-0.20944 0.20944"></position>
        </default>
        <default class="thmiddle">
          <joint axis="0 -1 0" range="-0.698132 0.698132"></joint>
          <position kp="1.5" ctrlrange="-0.698132 0.698132"></position>
        </default>
        <default class="thdistal">
          <joint range="-0.261799 1.5708"></joint>
          <position ctrlrange="-0.261799 1.5708"></position>
        </default>
      </default>

      <default class="metacarpal">
        <joint axis="0.573576 0 0.819152" range="0 0.785398"></joint>
        <position ctrlrange="0 0.785398"></position>
      </default>
      <default class="knuckle">
        <joint axis="0 -1 0" range="-0.349066 0.349066"></joint>
        <position ctrlrange="-0.349066 0.349066"></position>
      </default>
      <default class="proximal">
        <joint range="-0.261799 1.5708"></joint>
        <position ctrlrange="-0.261799 1.5708"></position>
      </default>
      <default class="middle_distal">
        <joint range="0 1.5708"></joint>
        <position kp="0.5" ctrlrange="0 3.1415"></position>
      </default>

      <default class="plastic">
        <geom solimp="0.5 0.99 0.0001" solref="0.005 1"></geom>
        <default class="plastic_visual">
          <geom type="mesh" material="black" contype="0" conaffinity="0" group="2"></geom>
        </default>
        <default class="plastic_collision">
          <geom group="3"></geom>
        </default>
      </default>
    </default>
  </default>

